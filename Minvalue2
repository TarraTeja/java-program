import java.util.*;
class Minvalue2
{
 public static void main(String arg[])
  {
   Scanner sc =new Scanner(System.in);
   int size,i;
   System.out.println("Enter array size");
   size=sc.nextInt();
   int a[]=new int[size];
   System.out.print("Enter elements");
   for(i=0;i<size;i++)
    {
     a[i]=sc.nextInt();
    }
  Arrays.sort(a);
  System.out.format("sorted %d",+a[0]);
  }
} 
    
